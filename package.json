{
  "name": "@specfocus/data-focus",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/specfocus/release.git"
  },
  "description": "React library with Recoil, using main/actions with async JSON and @mui data table",
  "author": "Lucas M. Oromi",
  "main": "index.js",
  "scripts": {
    "release": "node ../../scripts/publish react-focus ./release/packages",
    "publish": "npm build && npm market && cd dist && npm publish --access public",
    "market": "node ./scripts/copy-package-json",
    "prebuild-skip": "tslint -c tslint.json -p tsconfig.json --fix",
    "clean": "rimraf ./dist",
    "build": "tsc",
    "cra-start": "react-scripts start",
    "cra-build": "react-scripts build",
    "cra-test": "react-scripts test",
    "cra-eject": "react-scripts eject",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@specfocus/json-focus": "^0.0.1",
    "@specfocus/main-focus": "^0.0.1",
    "@specfocus/quiz-focus": "^0.0.1"
  },
  "devDependencies": {
    "@mui/lab": "^5.0.0-alpha.61",
    "@mui/material": "^5.2.6",
    "@mui/styles": "^5.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/history": "^4.7.9",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.31",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "date-fns": "^2.28.0",
    "jest": "^27.4.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.0",
    "recoil": "^0.7.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.5.0",
    "webpack": "^5.65.0"
  },
  "peerDependencies": {
    "@mui/lab": ">= 5.0.0-alpha.61",
    "@mui/material": ">= 5.x",
    "@mui/styles": ">= 5.x",
    "@specfocus/main-focus": ">= 0.0.3",
    "date-fns": ">= 2.x",
    "react": ">= 18.x",
    "react-dom": ">= 18.x",
    "recoil": ">= 0.5.x"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
